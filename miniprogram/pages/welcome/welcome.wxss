page {
  padding-bottom: 50rpx;
  background-color: #aadbd9;
}
.UCenter-bg {
  background-size: cover;
  height: 550rpx;
  display: flex;
  justify-content: center;
  padding-top: 40rpx;
  overflow: hidden;
  position: relative;
  flex-direction: column;
  align-items: center;
  color: #fff;
  font-weight: 300;
  text-shadow: 0 0 3px rgba(0, 0, 0, 0.3);
}

.UCenter-bg text {
  opacity: 0.8;
}

.UCenter-bg image {
  width: 200rpx;
  height: 200rpx;
}

.UCenter-bg .gif-wave{
  position: absolute;
  width: 100%;
  bottom: 0;
  left: 0;
  z-index: 99;
  mix-blend-mode: screen;  
  height: 100rpx;   
}

map,.mapBox{
  left: 0;
  z-index: 99;
  mix-blend-mode: screen;  
  height: 100rpx;   
}

map,.mapBox{
  width: 750rpx;
  height: 300rpx;
}
.result{
  /* background-color: darkgray; */
  display: flex;
  justify-content: space-around;
 flex-direction: column;
 align-items: center;
  height: 150rpx;
 width:100%;
 margin:2px auto;
 /* background-color: rgb(192, 214, 214); */
 border-bottom: 30rpx solid #edededad;
 border-top: 30rpx solid #edededad;
}
.header {
  padding: 0rpx;
  padding-top: 40rpx;
  padding-bottom: 50rpx;
  width: 107%;
}
.header2 {
  padding-left: 20rpx;
  padding-top: 30rpx;
  padding-right: 20rpx;
  width: 100%;
}

.iimg{
  width: 45rpx;
