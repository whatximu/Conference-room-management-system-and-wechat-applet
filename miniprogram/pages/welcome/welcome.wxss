}

.user_box {
  display: flex;
  align-items: center;
  width: 94%;
  height: 115px;
  background: rgba(255,255,255,0.7);
}


.logo {
  margin-left: 20px;
  margin-right: 20px;
  width: 150rpx;
  height: 150rpx;
  border-radius: 50%;
  background: #fff;
}

.logo image {
  width: 100%;
  height: 100%;
  border-radius: 100%;
  will-change: transform;
}

.user_name {
  display: flex;
  align-items: center;
  font-size: 36rpx;
